package com.zhy.drift.exception;

/**
 * DAO层异常
 * @author zhy
 *
 */
public class NullRecordsException extends Exception  {

    /**
     * 
     */
    private static final long serialVersionUID = -5713062850590349904L;

    public NullRecordsException() {

    }

    public NullRecordsException(String message) {
        super(message);
    }

    public NullRecordsException(String message, Throwable cause) {
        super(message, cause);
    }

    public NullRecordsException(Throwable cause) {
        super(cause);
    }
}
