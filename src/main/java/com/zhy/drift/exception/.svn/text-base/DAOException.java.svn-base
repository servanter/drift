package com.zhy.drift.exception;

/**
 * DAO层异常
 * @author zhy
 *
 */
public class DAOException extends Exception  {

    /**
     * 
     */
    private static final long serialVersionUID = -5713062850590349904L;

    public DAOException() {

    }

    public DAOException(String message) {
        super(message);
    }

    public DAOException(String message, Throwable cause) {
        super(message, cause);
    }

    public DAOException(Throwable cause) {
        super(cause);
    }
}
