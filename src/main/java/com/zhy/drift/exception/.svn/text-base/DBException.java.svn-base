package com.zhy.drift.exception;

/**
 * SQL&数据库异常
 * @author zhy
 *
 */
public class DBException extends Exception  {

    /**
     * 
     */
    private static final long serialVersionUID = -5713062850590349904L;

    public DBException() {

    }

    public DBException(String message) {
        super(message);
    }

    public DBException(String message, Throwable cause) {
        super(message, cause);
    }

    public DBException(Throwable cause) {
        super(cause);
    }
}
